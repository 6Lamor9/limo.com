---
const pd = {
    title: 'Limo Line',
    logo: 'https://avatars.githubusercontent.com/u/76264162?s=200&v=4',
    nav: [
        {
            title:"home",
            url:"/"
        },
        {
            title:"services",
            url:"/services"
        },
        {
            title:"Limo Fleet",
            url:"/fleet"
        },
        {
            title:"Book a Limo",
            url:"/book"
        }
    ],
    botton:"book a ride"
}
---


<div class="navbar bg-base-100 font-os z-50  fixed top-0 ">
    <div class="navbar-start ">
      <div class="dropdown">
        <label tabindex="0" class="btn btn-ghost lg:hidden">
          <svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5" fill="none" viewBox="0 0 24 24" stroke="currentColor"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 6h16M4 12h8m-8 6h16" /></svg>
        </label>
        <ul tabindex="0" class="menu menu-lg   decoration-slice text-lg dropdown-content mt-3 z-[1] p-2 shadow bg-base-100 rounded-box w-52">
            {
                pd.nav.map(e =>{
                    return <li><a href={e.url}>{e.title}</a></li>
                })
            }
        </ul>
      </div>
      <a class="btn btn-ghost normal-case text-2xl font-ax">{pd.title}</a>
    </div>
    <div class="navbar-center hidden lg:flex">
      <ul class="menu menu-horizontal text-lg  px-1">
        {
            pd.nav.map(e =>{
                return <li><a href={e.url}>{e.title}</a></li>
            })
        }
      </ul>
    </div>
    <div class="navbar-end ">
      <a class="btn bg-secondary text-black">{pd.botton}</a>
    </div>
  </div>